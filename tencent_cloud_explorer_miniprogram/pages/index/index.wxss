.body {
  margin: 10rpx 20rpx;
}

.box {
  padding: 0rpx 20rpx;
  border-top: 2px solid #000;
}

.cell {
  margin-top: 10rpx;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/**index.wxss**/
